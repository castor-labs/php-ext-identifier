{
    "runner.bootstrap": "vendor/autoload.php",
    "runner.path": "src",
    "runner.iterations": [10],
    "runner.revs": [1000],
    "runner.warmup": [2],
    "runner.retry_threshold": 5.0,
    "report.generators": {
        "table": {
            "cols": ["benchmark", "subject", "mode", "mean", "diff", "mem_peak"],
            "sort": {
                "mean": "asc"
            }
        },
        "html": {
            "template": "default"
        }
    },
    "storage.driver": "dbal",
    "storage.dbal.connection": {
        "driver": "pdo_sqlite",
        "path": "results/phpbench.db"
    }
}
